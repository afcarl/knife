#!MC 1000
$!ALTERDATA 
  EQUATION = '{u} = {xmom}/{rho}' 
$!ALTERDATA 
  EQUATION = '{v} = {ymom}/{rho}' 
$!ALTERDATA 
  EQUATION = '{w} = {zmom}/{rho}' 
$!ALTERDATA 
  EQUATION = '{vel} = sqrt({u}*{u} + {v}*{v} + {w}*{w})' 
$!ALTERDATA 
  EQUATION = '{gamma} = 1.4' 
$!ALTERDATA 
  EQUATION = '{p/pinf} = ({gamma}-1.0)*({energy} - 0.5*{rho}*{vel}*{vel})*{gamma}' 
$!ALTERDATA 
  EQUATION = '{c} = sqrt({p/pinf}/{rho})' 
$!ALTERDATA 
  EQUATION = '{mach} = {vel}/{c}' 
